{
  "compilerOptions": {
    "target": "es2015",
    "useDefineForClassFields": true,
    "module": "CommonJS",
    "lib": ["ES2020"],
    "moduleResolution": "Node",
    "strict": false,
    "sourceMap": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": false,
    "inlineSourceMap": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "jsx": "react",
    "noImplicitAny": false,
    "outDir": "out"
  }
}
